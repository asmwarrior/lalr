        libraries = {
            "sweet/lua/lua",
            "lua/liblua",
            "sweet/assert/assert",
            "sweet/error/error",
            "unit/libUnitTest"
        };

        build:Cxx () {
            "LuaUnitTest.cpp"
        };
    };
end
